// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource sqlite {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "sqlite"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String    @id @default(cuid())
  name      String    @default("")
  email     String    @unique @default("")
  password  String
  createdAt DateTime? @default(now())
}

model Client {
  id        String    @id @default(cuid())
  name      String    @default("")
  phone     String    @default("")
  question  String    @default("")
  createdAt DateTime? @default(now())
}

model Title {
  id          String    @id @default(cuid())
  name        String    @default("")
  details     String    @default("")
  title       String    @default("")
  description String    @default("")
  createdAt   DateTime? @default(now())
}

model Document {
  id          String    @id @default(cuid())
  slug        String    @unique @default("")
  title       String    @default("")
  description String    @default("")
  content     String    @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  createdAt   DateTime? @default(now())
  updatedAt   DateTime? @updatedAt
}

model Contact {
  id        String    @id @default(cuid())
  name      String    @default("")
  value     String    @default("")
  createdAt DateTime? @default(now())
}

model About {
  id        String    @id @default(cuid())
  title     String    @default("")
  text      String    @default("")
  createdAt DateTime? @default(now())
}

model Statistic {
  id        String    @id @default(cuid())
  title     String    @default("")
  text      String    @default("")
  createdAt DateTime? @default(now())
}

model Possibilitie {
  id        String              @id @default(cuid())
  title     String              @default("")
  text      String              @default("")
  points    PossibilitiePoint[] @relation("PossibilitiePoint_possibilitie")
  createdAt DateTime?           @default(now())
}

model Stage {
  id        String       @id @default(cuid())
  title     String       @default("")
  text      String       @default("")
  happening StagePoint[] @relation("StagePoint_stage")
  createdAt DateTime?    @default(now())
}

model Case {
  id        String    @id @default(cuid())
  direction String    @default("")
  title     String    @default("")
  text      String    @default("")
  solution  String    @default("")
  effect    String    @default("")
  createdAt DateTime? @default(now())
}

model Faq {
  id        String    @id @default(cuid())
  question  String    @default("")
  answer    String    @default("")
  createdAt DateTime? @default(now())
}

model PossibilitiePoint {
  id             String        @id @default(cuid())
  name           String        @default("")
  possibilitie   Possibilitie? @relation("PossibilitiePoint_possibilitie", fields: [possibilitieId], references: [id])
  possibilitieId String?       @map("possibilitie")

  @@index([possibilitieId])
}

model StagePoint {
  id      String  @id @default(cuid())
  name    String  @default("")
  stage   Stage?  @relation("StagePoint_stage", fields: [stageId], references: [id])
  stageId String? @map("stage")

  @@index([stageId])
}
